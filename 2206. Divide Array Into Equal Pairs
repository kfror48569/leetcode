class Solution {
public:
    bool divideArray(vector<int>& nums) {
    	vector <int> v;
        bool t;
        int arr[1000];
        for(int i=0;i<nums.size();i++){
            if (count(nums.begin(),nums.end(),nums[i])%2==0){
                cout<<count(nums.begin(),nums.end(),nums[i])<<" "<<endl;
                t=true;
            }else{
                return false;
            }
        }



        return t;

    }

};
